<script type="text/javascript">
  $(document).ready(function () {
    console.log("hello")
    $('.chat').hide()
    $('#chatbutton').click(function () {
      $('.chat').show()
    })
    $('#closebutton').click(function () {
      $('.chat').hide()
    })



    $('.filtering').slick({
    slidesToShow: 4,
    slidesToScroll: 4,
    dots: true,
    arrows:true,
    nextArrow: $('.next'),
    prevArrow: $('.prev'),
    });
    console.log("first level complete")
    var filtered = false;

    $('.js-filter').on('click', function () {
      if (filtered === false) {
        $('.filtering').slick('slickFilter', ':even');
        $(this).text('Unfilter Slides');
        filtered = true;
      } else {
        $('.filtering').slick('slickUnfilter');
        $(this).text('Filter Slides');
        filtered = false;
      }
    });

  });
</script>

