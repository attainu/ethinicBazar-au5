<div class="row" style="margin-left:56px;">
    <div class=" col-2 col-md-2" style=" background:white; margin-top:53px; margin-right:100px;">
        <section style="">
            <div class="condition-render-verticalFilters condition-render-hung" style="top: 180.4px; bottom: auto; ">
                <div class="vertical-filters-filters header-container"
                    style="background: white; width: 270px; margin-left: 20px; border:1px solid whitesmoke;">
                    <span class="header-title">FILTERS</span>

                </div>

                <div class="vertical-filters-filters categories-container"
                    style="background: white; width: 270px; margin-left: 20px; border:1px solid whitesmoke;">
                    <hr>
                    <span class="vertical-filters-header">Categories</span>
                    <div class="filter-search-filterSearchBox filter-search-expanded">
                        <input type="text" class="filter-search-inputBox" placeholder="Search for Categories"
                            style="height: 35px;">
                        <span class="myntraweb-sprite filter-search-closeSearch sprites-remove"></span>
                    </div>
                    <ul class="categories-list">

                        {{#eachUnique items}}
                        <li><label class="custom-control custom-checkbox"><input type="checkbox"
                                    class="custom-control-input" value="{{subCategory}}" name="category"><span
                                    class="custom-control-indicator"></span><!-- react-text: 38 --> {{subCategory}}
                                <!-- /react-text --><span class="categories-num">
                                    <!-- react-text: 40 -->
                                    <!-- /react-text --></span>
                                <div class="common-checkboxIndicator"></div>
                            </label></li>

                        {{/eachUnique}}

                    </ul>


                </div>
                <div class="vertical-filters-filters brand-container"
                    style="background: white; width: 270px; margin-left: 20px; border:1px solid whitesmoke">
                    <span class="vertical-filters-header">Product Name</span>
                    <div class="filter-search-filterSearchBox">
                        <input type="text" class="filter-search-inputBox filter-search-hidden"
                            placeholder="Search for Brand" style="height: 35px;">
                        <span class="myntraweb-sprite filter-search-iconSearch sprites-search"></span>
                    </div>
                    <ul class="brand-list">
                        {{#each items}}
                        <li><label class="custom-control custom-checkbox"><input type="checkbox" name="productName"
                                    class="custom-control-input filterproduct" value="{{productName}}" id="one"> <span
                                    class="custom-control-indicator"></span> <!-- react-text: 120 -->{{productName}}
                                <!-- /react-text --><span class="brand-num">
                                    <!-- react-text: 12<!-- /react-text --></span>
                                <div class="common-checkboxIndicator"></div>
                            </label></li>
                        {{/each}}
                    </ul>

                </div>

            </div>
        </section>
    </div>
    <div class=" col-8 col-md-8" style="background: white;  max-width:1300px;">
        <div>

            <ol class="breadcrumb col-12 col-sm-12" style="background-color:transparent;">
                <li><a href="#">Home/</a></li>
                <li><a href="#">Clothing/</a></li>
                <li><a href="#">{{items.0.subCategory}}</a></li>

            </ol>
        </div>


        <div class="row" id="filter">
            <br>
            <br>

            {{#each items}}


            <div class=" col-md-3  shadow1  ">

                <img src=" {{this.productImage}}" alt="" style="margin:-3px;"
                    onclick="window.location.href='/product/{{this._id}}'">

                <ul>
                    <li>
                        <p class="product-title ">{{this.productName}}</p>
                    </li>
                    <li>
                        <div>
                            <p class="discount-container" style="text-align: center;">

                                <span class="after-discount"> <strong
                                        style="font-family: Whitney-SemiBold, Whitney;color: #282C3F;font-size: 20px;">â‚¹{{this.productPrice}}</strong>
                                </span></b> </h2>

                            </p>
                        </div>
                    </li>

                </ul>


            </div>





            <br>
            <br>
            <br>
            {{/each}}



        </div>



    </div>

</div>


